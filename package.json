{
  "name": "t-ende-landing-page",
  "version": "0.1.0",
  "homepage": "https://dmitriinazimov.github.io/landing",
  "private": true,
  "dependencies": {
    "classnames": "2.3.2",
    "i18next": "23.4.6",
    "i18next-browser-languagedetector": "7.1.0",
    "i18next-http-backend": "2.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.12",
    "react-helmet": "6.1.0",
    "react-i18next": "13.2.2",
    "react-intersection-observer": "9.5.2",
    "react-router-dom": "6.16.0",
    "react-scripts": "5.0.1",
    "react-scroll": "1.9.0"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build -r https://github.com/dmitriinazimov/landing.git",
    "start": "react-scripts start",
    "build": "rm -rf build && react-scripts build && shx cp build/index.html build/404.html",
    "postbuild": "react-snap",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "8.48.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-i18next": "6.0.3",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "gh-pages": "6.1.0",
    "node-sass": "7.0.3",
    "prettier": "3.0.3",
    "react-snap": "1.23.0",
    "shx": "0.3.4",
    "stylelint": "15.10.3",
    "stylelint-config-standard-scss": "11.0.0"
  }
}
